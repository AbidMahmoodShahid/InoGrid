    <Color x:Key="ColorDiagramLimit">#DB0000</Color>
    <Color x:Key="ColorDiagramSectionSplitter">#969696</Color>
    <Color x:Key="ColorDiagramMajorLine">#969696</Color>
    <Color x:Key="ColorDiagramBorder">#000000</Color>
    <Color x:Key="ColorDiagramNeutral1">#FF8C8C</Color>
    <Color x:Key="ColorDiagramNeutral2">#5E9AE0</Color>

    <!--  Added Colors  -->
    <!--  Calendar Colors  -->
    <Color x:Key="ColorDarkCharcoal">#FF333333</Color>
    <Color x:Key="ColorDarkGray">#FF777777</Color>
    <Color x:Key="ColorDisabledColor">#A5FFFFFF</Color>
    <!--  Button Colors  -->
    <Color x:Key="ColorButtonBrownColor">#60000000</Color>

</ResourceDictionary>